# Не дотримання принципу розділення інтерфейсу
class Worker:
    def work(self):
        pass

    def eat(self):
        pass

# Дотримання принципу розділення інтерфейсу
class Workable:
    def work(self):
        pass

class Eatable:
    def eat(self):
        pass

class Worker(Workable, Eatable):
    pass
